--ROCK HUB W--
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("ROCK", "LightTheme")

-- Scripts Tab --
local Scripts = Window:NewTab("Scripts")
-- YBA --
local YbaSection = Scripts:NewSection("YBA")

YbaSection:NewButton("B boy hub", "B### boy hub fr", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/zakater5/LuaRepo/main/YBA/MainGui.lua"))()
end)

YbaSection:NewButton("Yba cus yes", "Yba cus yes mhm", function()
    getgenv().Key = "HelloEpicGUI"
    loadstring(game:HttpGet("https://raw.githubusercontent.com/SupBabe/YBAHOPPER/main/YBAv2.lua"))()
end)

YbaSection:NewButton("black gui re written", "black >:)", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Tobias020108Back/YBA-AUT/main/YBA-GUI-Rewrite.lua"))() 
end)

YbaSection:NewButton("Nuke Hub", "nuke incoming BEEP BEEP BEEP", function()
    _G.Key = "key"
    loadstring(game:HttpGet("https://raw.githubusercontent.com/NukeVsCity/TheALLHACKLoader/main/NukeLoader"))()
end)

